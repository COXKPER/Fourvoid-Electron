{
  "name": "fourvoid-electron",
  "version": "26.2.21",
  "description": "Official Fourvoid Desktop Client",
  "main": "main.js",
  "build": {
    "appId": "com.bgunevoid.fourvoid",
      "publish": [
      {
        "provider": "github",
        "owner": "COXKPER",
        "repo": "Fourvoid-Electron"
      }
    ],
    "productName": "Fourvoid",
    "files": [
      "**/*"
    ],
    "protocols": [
    {
      "name": "Fourvoid Protocol",
      "schemes": ["fourvoid"]
    }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "build/icon.png"
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "PT Bgunevoid Angkasa",
  "license": "ISC",
  "devDependencies": {
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3"
  }
}
